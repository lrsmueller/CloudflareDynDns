{
    "version": "2.0",
    "aggregator": {
        "batchSize": 1000,
        "flushTimeout": "00:00:30"
    },
    "logging": {
        "fileLoggingMode": "debugOnly",
        "logLevel": {
            "default": "Warning",
            "Host.Aggregator": "Trace",
            "Host.Results": "Information",
            "Function": "Information"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond": 5,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": { "routePrefix": "" }
    }
}